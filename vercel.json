{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "@vercel/php"
        },
        {
            "src": "vite.config.js",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}
